{
    "name": "route-httpbin",
    "customer": "contoso",
    "hosts": [
        "contoso.com"
    ],
    "paths": [
        "/"
    ],
    "incoming_schemes": [
        "Http",
        "Https"
    ],
    "outgoing_scheme": "MatchIncoming",
    "origin_group": {
        "origins": [
            {
                "host": "httpbin.org",
                "host_header_override": "httpbin.org",
                "sni": "httpbin.org",
                "weight": 100
            }
        ]
    }
}